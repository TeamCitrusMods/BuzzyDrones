{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${group}",
    "id": "buzzydrones",
    "version": "${version}",
    "name": "Buzzy Drones",
    "description": "A Minecraft Mod that adds drones to move items between containers",
    "authors": ["HexagonNico", "ZestyBlaze"],
    "contact": {
      "homepage": "",
      "sources": "",
      "issues": ""
    },
    "license": "Attribution-NonCommercial-ShareAlike 4.0 International",
    "icon": "assets/buzzydrones/icon.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "net.buzzydrones.quilt.BuzzyDronesQuilt"
      ],
      "client_init": [
        "net.buzzydrones.quilt.BuzzyDronesQuiltClient"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.19.2"
